@model IEnumerable<Dictamenes.Models.TipoDictamen>

@{
    ViewBag.Title = "Index";
}

<h2>Tipos de Dictamen</h2>

<p class="">
    <a class="btn btn-primary" href="@Url.Action("Create")">Cargar nuevo</a>
</p>
<table class="table table-striped table-hover">
    <thead>
        <tr class="table-primary">
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstaHabilitado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EstaHabilitado)
                </td>
                <td>
                    <a class="btn btn-outline-warning" href="@Url.Action("Edit", new { id = item.Id })"><i class="bi bi-pencil-square"></i></a>
                </td>
            </tr>
        }
    </tbody>

   

</table>
